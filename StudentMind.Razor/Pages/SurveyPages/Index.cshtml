@page
@model StudentMind.Razor.Pages.SurveyPages.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Survey[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Survey[0].Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Survey[0].StartDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Survey[0].EndDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Survey[0].TotalParticipants)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Survey[0].Type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Survey[0].CreatedBy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Survey[0].LastUpdatedBy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Survey[0].DeletedBy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Survey[0].CreatedTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Survey[0].LastUpdatedTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Survey[0].DeletedTime)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Survey) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalParticipants)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Type.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastUpdatedBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeletedBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastUpdatedTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeletedTime)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
